class product{
    String pcode,pname;
    int price;
    product(String pcode,String pname,int price){
        this.pcode=pcode;
        this.pname=pname;
        this.price=price;
    }
}


public class JavaApplication22 {

    public static void main(String[] args) {
        // TODO code application logic here
        product p1=new product("1","abc",25);
        
         product p2=new product("2","abcd",35);
         
          product p3=new product("3","abcde",45);
          
          if(p1.price <p2.price && p1.price<p3.price){
              System.out.println(p1.pname+" is smaller.");
          }
          else if(p2.price<p3.price){
           System.out.println(p2.pname+" is smaller.");   
          }
          else{
              System.out.println(p3.pname+" is smaller.");
          }
    }
    
}
