import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int[] a=new int[20];
		int s=5;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the elements :");
		for(int i=0;i<s;i++){
		    a[i]=sc.nextInt();
		}
	 System.out.println("printing elements of an array (Scanner class):");
     for(int i=0;i<s;i++){
     System.out.print(" "+ a[i]);
     }

	}
}
